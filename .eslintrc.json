{
  "extends": ["next/core-web-vitals", "next/typescript", "plugin:prettier/recommended", "@feature-sliced"],
  "parser": "@typescript-eslint/parser",
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  },
  "rules": {
    "@typescript-eslint/no-empty-interface": "off",
    "@typescript-eslint/no-empty-object-type": "off",
    "react/display-name": "warn"
  }
}
